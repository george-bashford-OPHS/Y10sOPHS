number = int(input("Enter a number 1, 2 or 3  "))
if number == 1:
             print("Thank you")
if number == 2:
             print("Well done, you're so smart")
if number == 3:
             print("Amazing, you are so good at this")
if number != 1 and number != 2 and number != 3:
    print("Really? What is wrong with you!?!?!?!?!?!?!?!?!?!!??!?!!?")
